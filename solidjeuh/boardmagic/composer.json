{
	"name": "solidjeuh/boardmagic",
	"type": "phpbb-extension",
	"description": "Met deze extensie kan je template events, css & taal strings toevoegen of aanpassen - With this extension you can add or edit template events, css & language strings",
	"homepage": "https://www.onnozelmanneke.space",
	"version": "1.0.5",
	"time": "2021-02-18",
	"keywords": [
		"phpbb",
		"extension",
		"boardmagic"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Onnozel Manneke",
			"homepage": "https://www.onnozelmanneke.space",
			"email": "info@onnozelmanneke.space",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Board Magic",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.3"
		},
		"version-check": {
            "host": "www.onnozelmanneke.space",
